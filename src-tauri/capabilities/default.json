{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "deep-link:default",
    "shell:default",
	{
		"identifier": "shell:allow-execute",
		"allow": [
			{
				"name": "exec-sh",
				"cmd": "sh",
				"args": [
					"-c",
					{
						"validator": "\\S+"
					}
				],
				"sidecar": false
			}
		]
	}
  ]
}
